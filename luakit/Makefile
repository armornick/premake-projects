ifndef config
  config=release32
endif
export config

PROJECTS := Prototype1 LuaKit

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

Prototype1: 
	@echo "==== Building Prototype1 ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f Prototype1.make

LuaKit: 
	@echo "==== Building LuaKit ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f LuaKit.make

clean:
	@${MAKE} --no-print-directory -C . -f Prototype1.make clean
	@${MAKE} --no-print-directory -C . -f LuaKit.make clean

help:
